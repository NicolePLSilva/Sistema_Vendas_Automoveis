@using SistemasVendasDeAutomoveis.Enums
@model CarroModel
@{
    ViewData["Title"] = Model.Marca +" "+ Model.Modelo+ " Detalhes";
}
<div class="container mt-5 mb-5">
    <div class="row d-flex justify-content-center border bg-success" style="padding:7px; box-shadow:0px 0px 4px 2px black">
        <div class="col-md-6">
            <div class="card bg-body">
                <img src="data:image/png;base64," class="card-img-top" alt="...">
                <div class="card-text"><small class="text-muted">Descrição: @Model.Descricao</small></div>
            </div>
        </div>
        <div class="col-md-6 ">
            
            <div class="mt-4 mb-3">
                
                <h3 class="text-uppercase">@Model.Marca @Model.Modelo @Model.Ano</h3>
                <div class="card-body font-monospace fw-bolder fs-5">
                    <div class="card-text ">Tipo 
                        @if (Model.Carroceria == CarroceriaEnum.COUPE)
                        {
                            <span>COUPÉ</span>
                        }
                        else if(Model.Carroceria == CarroceriaEnum.SEDA)
                        {
                            <span>SEDÃ</span>
                        }
                        else if(Model.Carroceria == CarroceriaEnum.FURGAO)
                        {
                            <span>FURGÃO</span>
                        }
                        else if (Model.Carroceria == CarroceriaEnum.CONVERSIVEL)
                        {
                            <span>CONVERSÍVEL</span>
                        }
                        else if (Model.Carroceria == CarroceriaEnum.ULTILITARIO)
                        {
                            <span>UTILITÁRIO</span>
                        }
                        else
                        {
                            <span>@Model.Carroceria</span>
                        }
                    </div>
                    <br/>
                    <div class="card-text">Câmbio 
                        @if (@Model.Cambio == CambioEnum.AUTOMATICO)
                        {
                            <span>AUTOMÁTICO</span>
                        }
                        else if (@Model.Cambio == CambioEnum.SEMIAUTOMATICO)
                        {
                            <span>SEMI-AUTOMÁTICO</span>
                        }
                        else
                        {
                            <span>@Model.Cambio</span>
                        }
                    </div>
                    <br />
                    <div class="card-text ">Combustível
                        @if(Model.Combustivel == CombustivelEnum.ALCOOL)
                        {
                            <span>ÁLCOOL</span>
                        }
                        else if(Model.Combustivel == CombustivelEnum.HIBRIDO)
                        {
                            <span>HÍBRIDO</span>
                        }
                        else if (Model.Combustivel == CombustivelEnum.ELETRICO)
                        {
                            <span>ELÉTRICO</span>
                        }
                        else
                        {
                            <span>@Model.Combustivel</span>
                        }
                    </div>
                    <div class="card-text">Cor: @Model.Cor</div>
                    @if(Model.Estado == EstadoEnum.NOVO)
                    {
                        <div class="card-text">Estado: NOVO</div>
                        <div class="card-text">Veículo Zero KM</div>
                    }
                    else
                    {
                        <div class="card-text">Estado: USADO</div>
                        <div class="card-text">@Model.Quilometragem KM rodados</div>
                    }
                    
                    <div class="card-text act-price text-center">R$@Model.Preco</div>
                </div>
            </div>
            
        </div>

    </div>
    <small class="text-black">Voltar</small>
</div>